@article{boresch_avoiding_2011,
	title = {Avoiding the van der {Waals} endpoint problem using serial atomic insertion},
	volume = {32},
	issn = {1096-987X},
	url = {http://onlinelibrary.wiley.com/doi/abs/10.1002/jcc.21829},
	doi = {10.1002/jcc.21829},
	abstract = {In the past analyses of the so-called van der Waals end point problem focused on thermodynamic integration. Here we investigate which of the recommendations, such as the need for soft-core potentials, are still valid when Bennett's acceptance ratio method is used. We show that in combination with Bennett's acceptance ratio method intermediate states characterized by the coupling parameter λ can be replaced by intermediate states in which Lennard-Jones interactions are turned on or off on an “atom by atom” basis. By doing so, there is no necessity to use soft-core potentials. In fact, one can compute free energy differences without dedicated code, making it possible to use any molecular dynamics program to compute alchemical free energy differences. Such an approach, which we illustrate by several examples, makes it possible to exploit the tremendous computational power of the graphics processing unit. © 2011 Wiley Periodicals, Inc. J Comput Chem, 2011},
	language = {en},
	number = {11},
	urldate = {2022-07-07},
	journal = {Journal of Computational Chemistry},
	author = {Boresch, Stefan and Bruckner, Stefan},
	year = {2011},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/jcc.21829},
	keywords = {Bennett's acceptance ratio methods, GPU computing, soft-core-potential, van der Waals endpoint problem},
	pages = {2449--2458},
}

